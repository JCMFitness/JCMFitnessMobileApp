<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="JCMFitnessMobileApp.Views.LandingPage">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" VerticalOptions="Center" HorizontalOptions="Center">
            <Button Text="Login" Command="{Binding LoginCommand}"></Button>
            <Button Text="Sign up" Command="{Binding SignupCommand}"></Button>

           
            <Button Text="Google" Command="{Binding GoogleCommand}" BackgroundColor="#d34836" TextColor="White" />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>